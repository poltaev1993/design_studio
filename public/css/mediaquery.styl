			background-color rgba(0, 0, 0, 0.8)
			margin-left 0.05%
			width 95.95%
			padding 4.3px 2%
	.close-btn
		position absolute
		top 20px
		right 20px
		width 20px
		height 20px
		font-size 20px
		color #fff
		cursor pointer
		display none
		&:hover
			color #fff
	.navburger-btn
		display none
		background-image url(../img/navburger.png)
		background-position center center
		background-repeat no-repeat
		background-size 15px
		height 28px
		background-color rgba(0, 0, 0, 0.8)
		margin-left 0.1%
		width 95.9%
		// padding 5.3px 2%
		cursor pointer
	.right-header
		width 70%
	.left-h
		width 30%
	.mixit-griding
		.grid-item
		.grid-gap
			display inline-block
			width 20%
