		margin 0 20px
		&.header_perfect_scrroll__js
			overflow visible !important
			.ps-scrollbar-x-rail
			.ps-scrollbar-y-rail
				display none !important
		.list
			padding 0
			margin 0
			float right
			li
				list-style none
				float left
				margin 0 10px
				a
					position relative
					color #fff
					font-size fsize(13)em
					font-family raleway_light, sans-serif
					transition color 0.3s
					&:hover
					&.active
						color #bbb

